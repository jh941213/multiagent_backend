   "source": [
    "import os\n",
    "import logging\n",
    "import yfinance as yf\n",
    "import psycopg2\n",
    "from dotenv import load_dotenv\n",
    "import json\n",
    "\n",
    "# 지수 심볼 정의\n",
    "INDICES = {\n",
    "    \"나스닥\": \"NDX\",  
    "    \"S&P 500\": \"SPX\",  
    "    \"다우존스\": \"DJI\",  
    "    \"VIX\": \"VIX\" 
    "}\n",
    "\n",
    "# 빅테크 주식 심볼 정의\n",
    "STOCKS = {\n",
    "    \"Apple\": \"AAPL\",\n",
    "    \"Microsoft\": \"MSFT\", \n",
    "    \"Tesla\": \"TSLA\",\n",
    "    \"Meta\": \"META\",\n",
    "    \"NVIDIA\": \"NVDA\",\n",
    "    \"Alphabet\": \"GOOGL\",\n",
    "    \"Amazon\": \"AMZN\"\n",
    "}\n",
    "\n",
    "# 환경변수 로드\n",
    "load_dotenv()\n",
    "\n",
    "# 로깅 설정\n",
    "logging.basicConfig(level=logging.INFO)\n",
    "logger = logging.getLogger(__name__)\n",
    "\n",
    "# PostgreSQL 연결 설정\n",
    "DB_CONFIG = {\n",
    "    'dbname': os.getenv('PSQL_DATABASE'),\n",
    "    'user': os.getenv('PSQL_USERNAME'),\n",
    "    'password': os.getenv('PSQL_PASSWORD'),\n",
    "    'host': os.getenv('PSQL_HOST'),\n",
    "    'port': os.getenv('PSQL_PORT')\n",
    "}\n",
    "\n",
    "def get_market_data(ticker, period=\"1d\", interval=\"5m\"):\n",
    "    \"\"\"\n",
    "    주가 데이터를 가져오는 함수\n",
    "    period: 데이터 기간 (1d, 5d, 1mo 등)\n",
    "    interval: 시간 간격 (1m, 2m, 5m, 15m, 30m, 60m, 90m, 1h, 1d, 5d, 1wk, 1mo, 3mo)\n",
    "    \"\"\"\n",
    "    try:\n",
    "        hist = ticker.history(period=period, interval=interval)\n",
    "        if not hist.empty:\n",
    "            current_price = hist['Close'].iloc[-1]\n",
    "            price_change = hist['Close'].iloc[-1] - hist['Open'].iloc[0]\n",
    "            change_percent = (price_change / hist['Open'].iloc[0]) * 100\n",
    "            \n",
    "            # 차트 데이터 생성 (시계열 데이터)\n",
    "            chart_data = [\n",
    "                {\n",
    "                    'time': str(idx.strftime('%Y-%m-%d %H:%M:%S')),  # ISO 형식의 시간\n",
    "                    'value': float(row['Close']),  # 종가\n",
    "                    'open': float(row['Open']),    # 시가\n",
    "                    'high': float(row['High']),    # 고가\n",
    "                    'low': float(row['Low']),      # 저가\n",
    "                    'volume': float(row['Volume']) # 거래량\n",
    "                } for idx, row in hist.iterrows()\n",
    "            ]\n",
    "            \n",
    "            return {\n",
    "                'current': float(current_price),\n",
    "                'change': float(price_change),\n",
    "                'changePercent': float(change_percent),\n",
    "                'data': chart_data\n",
    "            }\n",
    "    except Exception as e:\n",
    "        logger.error(f\"데이터 조회 오류: {str(e)}\")\n",
    "        return None\n",
    "\n",
    "def update_market_data():\n",
    "    logger.info(\"시장 데이터 업데이트 시작...\")\n",
    "    try:\n",
    "        # PostgreSQL 연결\n",
    "        conn = psycopg2.connect(**DB_CONFIG)\n",
    "        cur = conn.cursor()\n",
    "        \n",
    "        # 모든 시장 데이터를 저장할 리스트\n",
    "        market_data = []\n",
    "        \n",
    "        # 지수 데이터 업데이트 (KOSPI, KOSDAQ 포함)\n",
    "        indices = {\n",
    "            **INDICES,\n",
    "            \"KOSPI\": \"KS11\",\n",
    "            \"KOSDAQ\": \"KQ11\"\n",
    "        }\n",
    "        \n",
    "        # 지수 데이터 업데이트 부분\n",
    "        for name, symbol in indices.items():\n",
    "            try:\n",
    "                ticker = yf.Ticker(f\"^{symbol}\")\n",
    "                market_info = get_market_data(ticker, period=\"1d\", interval=\"5m\")\n",
    "                \n",
    "                if market_info:\n",
    "                    market_data.append({\n",
    "                        'name': name,\n",
    "                        'symbol': symbol,\n",
    "                        'current': market_info['current'],\n",
    "                        'change': market_info['change'],\n",
    "                        'changePercent': market_info['changePercent'],\n",
    "                        'flag': 'down' if market_info['change'] < 0 else 'up',\n",
    "                        'color': '#ef4444' if market_info['change'] < 0 else '#22c55e',\n",
    "                        'data': market_info['data'],\n",
    "                        'is_index': True\n",
    "                    })\n",
    "                    logger.info(f\"지수 업데이트 완료: {name} ({symbol})\")\n",
    "                    \n",
    "            except Exception as e:\n",
    "                logger.error(f\"지수 업데이트 오류 {symbol}: {str(e)}\")\n",
    "                continue\n",
    "\n",
    "        # 빅테크 주식 데이터 업데이트 부분\n",
    "        for name, symbol in STOCKS.items():\n",
    "            try:\n",
    "                ticker = yf.Ticker(symbol)\n",
    "                market_info = get_market_data(ticker, period=\"1d\", interval=\"5m\")\n",
    "                \n",
    "                if market_info:\n",
    "                    market_data.append({\n",
    "                        'name': name,\n",
    "                        'symbol': symbol,\n",
    "                        'current': market_info['current'],\n",
    "                        'change': market_info['change'],\n",
    "                        'changePercent': market_info['changePercent'],\n",
    "                        'flag': 'down' if market_info['change'] < 0 else 'up',\n",
    "                        'color': '#ef4444' if market_info['change'] < 0 else '#22c55e',\n",
    "                        'data': market_info['data'],\n",
    "                        'is_index': False\n",
    "                    })\n",
    "                    logger.info(f\"주식 업데이트 완료: {name} ({symbol})\")\n",
    "                    \n",
    "            except Exception as e:\n",
    "                logger.error(f\"주식 업데이트 오류 {symbol}: {str(e)}\")\n",
    "                continue\n",
    "\n",
    "        # 데이터베이스에 저장\n",
    "        try:\n",
    "            # market_data 테이블 업데이트\n",
    "            cur.execute(\"TRUNCATE TABLE market_data\")  # 기존 데이터 삭제\n",
    "            for data in market_data:\n",
    "                cur.execute(\"\"\"\n",
    "                    INSERT INTO market_data (\n",
    "                        name, symbol, current_price, price_change, \n",
    "                        change_percent, flag, color, chart_data, is_index\n",
    "                    ) VALUES (%s, %s, %s, %s, %s, %s, %s, %s, %s)\n",
    "                \"\"\", (\n",
    "                    data['name'], data['symbol'], data['current'], \n",
    "                    data['change'], data['changePercent'], data['flag'],\n",
    "                    data['color'], json.dumps(data['data']), data['is_index']\n",
    "                ))\n",
    "            \n",
    "            conn.commit()\n",
    "            logger.info(\"시장 데이터 업데이트가 성공적으로 완료되었습니다\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            conn.rollback()\n",
    "            logger.error(f\"데이터베이스 오류: {str(e)}\")\n",
    "            \n",
    "        finally:\n",
    "            cur.close()\n",
    "            conn.close()\n",
    "\n",
    "    except Exception as e:\n",
    "        logger.error(f\"업데이트 오류: {str(e)}\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    update_market_data()"
   ]
  },
